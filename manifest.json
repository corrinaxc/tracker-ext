{
    "manifest_version": 3,
    "name": "tracker-ext",
    "version": "1.0",
    "permissions": ["webNavigation", "tabs", "storage"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
        "default_popup": "index.html",
        "default_title": ""
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
        }
      ]
  }
  